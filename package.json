{
  "name": "@tanphat199/utilities",
  "private": true,
  "version": "0.0.1",
  "description": "Utilities",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "prebuild": "yarn lint",
    "build": "lb-tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn eslint && yarn prettier:check",
    "lint:fix": "yarn eslint --fix && yarn prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn prettier:cli",
    "prettier:fix": "yarn prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "clean": "sh clean.sh",
    "rebuild": "yarn clean && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phatnt199/utilities.git"
  },
  "author": "Phat Nguyen <tanphat199@gmail.com>",
  "license": "ISC",
  "files": [
    "README_DEV.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.3",
    "multer": "^1.4.5-lts.1",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.8",
    "@loopback/eslint-config": "^13.0.8",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.36",
    "@types/voca": "^1.4.2",
    "eslint": "^8.33.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "~4.9.4"
  }
}
