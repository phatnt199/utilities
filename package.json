  "dependencies": {
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.3",
    "lodash": "^4.17.21",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@loopback/eslint-config": "^13.0.9",
    "@types/crypto-js": "^4.1.1",
